@using K9.Base.WebApplication.Helpers
@model List<K9.WebApplication.Models.MembershipModel>

@{
    ViewBag.Title = @K9.Globalisation.Dictionary.MembershipOptions;
}

@Html.Partial("_SponsorIbogaBanner")
<div id="main-banner" class="banner-default main-menu">
    @Html.Partial("_Navbar")
</div>

<div class="main-section">
    <div class="container">
        <div class="main-heading">
            <h3>@K9.Globalisation.Dictionary.MembershipLabel</h3>
            <br/>
            <h4 class="text-black">@K9.Globalisation.Dictionary.BecomeBenefactorTitle</h4>
        </div>
    </div>
    <div class="container">
        <div class="text-center">
            @foreach (var membershipModel in Model)
            {
                @Html.Partial("_MembershipOption", membershipModel)
            }
        </div>
    </div>
</div>