@using K9.Base.WebApplication.Config
@using K9.Base.WebApplication.Helpers
@{
    ViewBag.Title = Dictionary.Home;
}
<div id="main-banner" class="banner-default main-menu">
    @Html.Partial("_Navbar")
</div>
<div class="main-section pink">
    <div class="container">
        <div class="main-heading">
            <h3>@WebsiteConfiguration.Instance.CompanyName C.I.C</h3>
        </div>
    </div>
    <div class="container">
        <div>
            <div class="col-md-5">
                <div class="picture-frame">
                    <div>
                        <img src="~/Images/home/wood1.jpg" alt="" />
                    </div>
                </div>
            </div>
            <div class="col-md-7 margin-top-minus-15">
                <article>
                    @MvcHtmlString.Create(K9.Globalisation.Dictionary.blessings_of_the_forest)
                </article>
            </div>
            <div class="clearfix">
            </div>
        </div>
    </div>
</div>
<div class="jarallax background-forest3 our-mission main-section">
    <div class="container scrollFadeIn">
        <div class="main-heading">
            <h3 class="text-white">@Dictionary.OurMissionTitle</h3>
        </div>
        <div class="">
            <div class="col-md-6">
                <div>
                    <h4>@K9.Globalisation.Dictionary.mission_statement_and_aims_title1</h4>
                </div>
                @MvcHtmlString.Create(K9.Globalisation.Dictionary.mission_statement_and_aims1)
            </div>
            <div class="col-md-6">
                <div>
                    <h4>@K9.Globalisation.Dictionary.mission_statement_and_aims_title2</h4>
                </div>
                @MvcHtmlString.Create(K9.Globalisation.Dictionary.mission_statement_and_aims2)
            </div>
            <div class="clearfix">
            </div>
        </div>
        <br />
        <div class="text-center">
            @Html.BootstrapActionLinkButton(Dictionary.SupportUs, "Index", "Home", null, "fa-thumbs-o-up", EButtonClass.Large)
        </div>
    </div>
</div>
<div class="main-section botf-context">
    <div class="container">
        <div class="main-heading">
            <h3>@K9.Globalisation.Dictionary.botf_context_title</h3>
        </div>
        <div class="container reveal-text">
            <div>
                <div class="col-md-12 scrollFadeIn">
                    <div class="picture-frame picture-frame-large-x pull-right margin-left-15">
                        <div>
                            <img src="~/Images/home/image8.jpg" alt="" />
                        </div>
                    </div>
                    <article class="margin-top-minus-15 clear-on-device scrollFadeIn">
                        @MvcHtmlString.Create(K9.Globalisation.Dictionary.botf_context1)
                    </article>
                </div>
                <div class="col-md-12 scrollFadeIn">
                    <div class="picture-frame picture-frame-large-x pull-left margin-right-15">
                        <div>
                            <img src="~/Images/home/image8.jpg" alt="" />
                        </div>
                    </div>
                    <article class="clear-on-device scrollFadeIn">
                        @MvcHtmlString.Create(K9.Globalisation.Dictionary.botf_context2)
                    </article>
                </div>
                <div class="col-md-12 scrollFadeIn">
                    <div class="picture-frame picture-frame-large-x pull-right margin-left-15">
                        <div>
                            <img src="~/Images/home/image8.jpg" alt="" />
                        </div>
                    </div>
                    <article class="clear-on-device scrollFadeIn">
                        @MvcHtmlString.Create(K9.Globalisation.Dictionary.botf_context3)
                    </article>
                </div>
                <div class="clearfix">
                </div>
            </div>
        </div>
    </div>
</div>
