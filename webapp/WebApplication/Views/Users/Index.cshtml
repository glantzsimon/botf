@using K9.Base.WebApplication.Helpers
@using K9.Base.WebApplication.Options
@model User
@{
	Layout = "~/Views/Shared/_LayoutFormWide.cshtml";
}
@Html.BootstrapTable(new DataTableOptions<User>
{
	 VisibleColumns = new List<string>
	 {
		 "Name",
		 "Username",
		 "BirthDate",
		 "EmailAddress",
		 "PhoneNumber",
		 "IsUnsubscribed"
	 }
})